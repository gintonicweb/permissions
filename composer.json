{
    "name": "gintonicweb/permissions",
    "description": "Permissions plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "cakephp/cakephp-codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Permissions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Permissions\\Test\\": "tests"
        }
    }
}
